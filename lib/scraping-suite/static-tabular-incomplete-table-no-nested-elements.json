
{
  "title": "Static - Tabular - Incomplete Table - No Nested Elements",
  
  "vhosts": [
    {
      "root": "./static-tabular",
      "hostname": "sitea.com"
    }
  ],
  
  "definition": {
    "url": "http://sitea.com/incomplete-table-no-nested-elements.html",
    "scenario": "static-content",
    "pluginOpts": [
      {
        "name": "ExtractHtmlContentPlugin",
        "selectorPairs": [
          {
            "selector": "table#t tr td:nth-child(1)"
          },
          {
            "selector": "table#t tr td:nth-child(2)"
          }
        ]
      }
    ]
  },

  "resources": [
    {
      "url": "http://sitea.com/incomplete-table-no-nested-elements.html",
      "contentType": "text/html",
      "content": { 
        "table#t tr td:nth-child(1)": [ "valA1", "valA2", "valA3" ],
        "table#t tr td:nth-child(2)": [ "valB1", "", "valB3" ]
      }
    }
  ]
}