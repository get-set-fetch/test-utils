
{
  "title": "Static - Multiple Pages - Multiple Content Entries - Max Resources 3 (partial save of newly discovered resources)",
  
  "vhosts": [
    {
      "root": "./static-multiple-pages",
      "hostname": "sitea.com"
    }
  ],
  
  "definition": {
    "url": "http://sitea.com/index.html",
    "scenario": "static-content",
    "pluginOpts": [
      {
        "name": "ExtractHtmlContentPlugin",
        "selectorPairs": [
          {
            "selector": "h1"
          },
          {
            "selector": "p.classA"
          }
        ]
      },
      {
        "name": "InsertResourcesPlugin",
        "maxResources": 3
      }
    ]
  },

  "resources": [
    {
      "url": "http://sitea.com/index.html",
      "contentType": "text/html",
      "content": [
        ["Main H1", "main p1"],
        ["Main H1", "main p2"]
      ]
    },
    {
      "url": "http://sitea.com/pageA.html",
      "contentType": "text/html",
      "content": [
        ["PageA H1", "pageA p1"],
        ["PageA H1", "pageA p2"]
      ]
    },
    {
      "url": "http://sitea.com/b/pageB.html",
      "contentType": "text/html",
      "content": [
        ["PageB H1", "pageB p1"],
        ["PageB H1", "pageB p2"]
      ]
    }
  ]
}