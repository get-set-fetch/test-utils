
{
  "title": "Static - Single Page - Multiple Content Entries",
  
  "vhosts": [
    {
      "root": "./static-single-page",
      "hostname": "sitea.com"
    }
  ],
  
  "definition": {
    "url": "http://sitea.com/index.html",
    "scenario": "static-content",
    "pluginOpts": [
      {
        "name": "ExtractHtmlContentPlugin",
        "selectorPairs": [
          {
            "selector": "h1"
          },
          {
            "selector": "i.classA"
          }
        ]
      }
    ]
  },

  "resources": [
    {
      "url": "http://sitea.com/index.html",
      "contentType": "text/html",
      "content": [
        ["Main Header 1", "italics 1"],
        ["Main Header 1", "italics 2"]
      ]
    }
  ]
}