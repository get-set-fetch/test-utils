
{
  "title": "Static - Single Page - Multiple Content Entries",
  
  "vhosts": [
    {
      "root": "./static-single-page",
      "hostname": "sitea.com"
    }
  ],
  
  "definition": {
    "url": "http://sitea.com/index.html",
    "scenario": "ScrapeStaticContent",
    "pluginOpts": [
      {
        "name": "ExtractHtmlContentPlugin",
        "selectorPairs": [
          {
            "selector": "h1"
          },
          {
            "selector": "i.classA"
          }
        ]
      }
    ]
  },

  "resources": [
    {
      "url": "http://sitea.com/index.html",
      "contentType": "text/html",
      "content": { 
        "h1": [ "Main Header 1","Main Header 1"],
        "i.classA": ["italics 1", "italics 2"]
      }
    }
  ]
}