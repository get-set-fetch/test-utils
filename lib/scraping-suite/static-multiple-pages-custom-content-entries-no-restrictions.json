
{
  "title": "Static - Multiple Pages - Custom Content Entries - No Restrictions",
  
  "vhosts": [
    {
      "root": "./static-multiple-pages",
      "hostname": "sitea.com"
    }
  ],
  
  "definition": {
    "url": "http://sitea.com/index.html",
    "scenario": "static-content",
    "pluginOpts": [
      {
        "name": "ExtractHtmlContentPlugin",
        "selectorPairs": [
          {
            "selector": "h1",
            "property": "title"
          },
          {
            "selector": "p.classA",
            "property": "data-p"
          }
        ]
      }
    ]
  },

  "resources": [
    {
      "url": "http://sitea.com/index.html",
      "contentType": "text/html",
      "content": [
        ["main title", "data main1"],
        ["main title", "data main2"]
      ]
    },
    {
      "url": "http://sitea.com/pageA.html",
      "contentType": "text/html",
      "content": [
        ["titleA", "data pageA1"],
        ["titleA", "data pageA2"]
      ]
    },
    {
      "url": "http://sitea.com/b/pageB.html",
      "contentType": "text/html",
      "content": [
        ["titleB", "data pageB1"],
        ["titleB", "data pageB2"]
      ]
    },
    {
      "url": "http://sitea.com/c/pageC.html",
      "contentType": "text/html",
      "content": [
        ["titleC", "data pageC1"],
        ["titleC", "data pageC2"]
      ]
    }
  ]
}