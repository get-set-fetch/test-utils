{
  "name": "get-set-fetch-test-utils",
  "version": "0.3.0",
  "description": "scraping test definitions, launches resources to be scraped under a configurable web server",
  "keywords": ["scraping", "test definition"],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "mocha --config test/.mocharc.js \"test/**/*.ts\"",
    "prebuild": "mkdir dist && cp -R lib/scraping-suite dist/scraping-suite && rm dist/scraping-suite/ScrapingSuite.ts",
    "build": "tsc"
  },
  "author": {
    "name": "Andrei Sabau"
  },
  "license": "MIT",
  "homepage": "https://github.com/get-set-fetch/test-utils",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/get-set-fetch/test-utils.git"
  },
  "devDependencies": {
    "@types/connect": "^3.4.33",
    "@types/jszip": "^3.4.1",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.7",
    "@types/serve-static": "^1.13.7",
    "@types/vhost": "^3.0.1",
    "mocha": "^8.1.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "connect": "^3.7.0",
    "jszip": "^3.5.0",
    "serve-static": "^1.14.1",
    "vhost": "^3.0.2"
  }
}
